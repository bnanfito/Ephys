% mergeExpts
clear all; close all;
dataFold = 'D:\data';
physDir = fullfile(dataFold,'Ephys');
units = {'000','000'};
expts = {'006','014'};
mergeID = [];
mergeID = zeros(1,6*length());
files{1,length(expts)} = [];
for e = 1:length(expts)
    mergeID = [mergeID units{e} expts{e}];
    files{e} = ['u' units{e} '_' expts{e}];
end
mergeIntan(physDir,'febg9',files,100,mergeID,'BRN')